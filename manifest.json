{
    "manifest_version": 2,
    "name": "Messanger",
    "version": "1.0.0",
    "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"]
}